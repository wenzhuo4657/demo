<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wenzhuo4657.mapper.userMapper">
    <select id="finadll" resultType="com.wenzhuo4657.domain.User">
        select *from user
    </select>

    <select id="login" resultType="com.wenzhuo4657.domain.userlogin">
        select *from  sys_user where username=#{username} and password=#{password}
    </select>
</mapper>